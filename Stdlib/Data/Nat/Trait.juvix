module Stdlib.Data.Nat.Trait;

import Stdlib.Trait.Eq as Eq;
open Eq using {Eq};

import Stdlib.Trait.Ord as Ord;
open Ord using {Ord};

import Stdlib.Trait.Show as Show;
open Show using {Show};

open import Stdlib.Data.Nat;

import Stdlib.Data.Nat.Ord as Nat;

module NatTrait;
  Eq : Eq Nat;
  Eq := Eq.eq (Nat.==);

  Ord : Ord Nat;
  Ord := Ord.ord Nat.compare;

  Show : Show Nat;
  Show := Show.show natToString;
end;
