module Stdlib.Data.String;

open import Stdlib.Data.Bool;

axiom String : Type;

compile String {
  c ↦ "char*";
};

foreign c {
  bool cStrEq(char* l, char* r) {
    return strcmp(l, r) == 0;
  \}
};

axiom eqString : String → String → BackendBool;
compile eqString  {
  c ↦ "eqString";
  ghc ↦ "(==)";
};

-- infix 4 backend==;
-- backend== : String → String → Bool;
-- backend== s1 s2 ≔ from-backend-bool (cStrEq s1 s2);

end;
