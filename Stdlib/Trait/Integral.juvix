module Stdlib.Trait.Integral;

import Stdlib.Data.Int.Base open using {Int};
import Stdlib.Data.Fixity open;
import Stdlib.Trait.Natural open;

trait
type Integral A :=
  mkIntegral {
    naturalI : Natural A;
    syntax operator - additive;
    {-# isabelle-operator: "-" #-}
    - : A -> A -> A;
    builtin from-int
    fromInt : Int -> A
  };

open Integral using {fromInt; -} public;
